global function long f_datetimetominutes (datetime a_datetime);// convertir l'heure contenue dans une variable DateTime en minutes
long	ll_minutes
time	lt_time

lt_time = time(a_datetime)
ll_minutes = hour(lt_time) * 60 + minute(lt_time)

return(ll_minutes)
end function

