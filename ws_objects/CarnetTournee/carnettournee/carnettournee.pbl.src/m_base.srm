public subroutine mf_enableaction (string as_itemstoenable[]);// disabler toutes les options du menu action (mais pas le menu action lui-même)
mf_disablemenu(this.m_action, FALSE)

// enabler les options voulues dans le menu action
mf_enableitems(this.m_action, as_itemstoenable)
end subroutine

public subroutine mf_hideaction (string as_itemstohide[]);// cacher les options voulues dans le menu action
mf_hideitems(this.m_action, as_itemstohide)
end subroutine

public subroutine mf_showaction (string as_itemstoshow[]);// afficher les options voulues dans le menu action
mf_showitems(this.m_action, as_itemstoshow)
end subroutine

public subroutine mf_otheritems ();// cocher ou pas l'option "traçage SQL" quand on change de menu, en fonction de l'état de la variable 'gb_sqlspy_on'
m_outils.m_sqlspy.checked = gb_sqlspy_on

end subroutine

public function integer mf_exportdico ();// export V_DICOPREST (nouveau référentiel) dans la langue de gs_langue
uo_ds		lds_dico
string	ls_filename, ls_message
boolean	lb_erreur

lb_erreur = FALSE

lds_dico = CREATE uo_ds
lds_dico.dataobject = "ds_referentiel_export"
lds_dico.setTransobject(SQLCA)
IF lds_dico.retrieve() > 0 THEN
	// fichier langue 1
	ls_filename = f_string(gs_cenpath + "\v_dicoprest" + upper(gs_langue) + ".xml")
	IF lds_dico.saveas(ls_filename, XML!, false) = 1 THEN
		ls_message = "Fichier " + ls_filename + " créé avec succès"
	ELSE
		ls_message = "Erreur création fichier " + ls_filename
		lb_erreur = TRUE
	END IF
ELSE
	ls_message = "Erreur retrieve lds_dico " + SQLCA.sqlerrtext
	lb_erreur = TRUE
END IF

DESTROY lds_dico
IF NOT lb_erreur THEN
	gu_message.uf_info(ls_message)
	return(1)
ELSE
	gu_message.uf_error(ls_message)
	return(-1)
END IF

end function

public function integer mf_showcellinfomsitems ();// cacher les options voulues dans le menu params, en fonction de l'appartenance au groupe "maintenance IT ou pas"
string	ls_fichier_items[] = {"m_param_1","m_param_2","m_traduction","m_detail_critere","m_report_critere", &
										 "m_report_predeftri","m_groupe","m_user","m_superuser","m_exporterversxml","m_cal_feries"}
string	ls_impressions_items[] = {"m_impressions_0","m_rpt_usergroups","m_rpt_users","m_rpt_userprivs",&
											  "m_rpt_progprivs","m_rpt_superprivs"}

mf_showitems(this.m_fichier.m_param, ls_fichier_items)
mf_showitems(this.m_impressions.m_impression_parametres, ls_impressions_items)

return(1)
end function

public function integer mf_exporttag ();// Export TAG (traductions) dans la langue de gs_langue.
// NB : on utilise le même DS que l'objet uo_translate.
uo_ds		lds_tag
string	ls_filename, ls_message
boolean	lb_erreur
long		ll_nbrows

lb_erreur = FALSE

lds_tag = CREATE uo_ds
lds_tag.dataobject = "ds_translations"
lds_tag.setTransobject(SQLCA)
IF upper(gs_langue) = "F" THEN
	ll_nbrows = lds_tag.retrieve("FR")
ELSE
	ll_nbrows = lds_tag.retrieve("DE")
END IF
IF ll_nbrows > 0 THEN
	ls_filename = f_string(gs_cenpath + "\tag" + upper(gs_langue) + ".xml")
	IF lds_tag.saveas(ls_filename, XML!, false) = 1 THEN
		ls_message = "Fichier " + ls_filename + " créé avec succès"
	ELSE
		ls_message = "Erreur création fichier " + ls_filename
		lb_erreur = TRUE
	END IF
ELSE
	ls_message = "Erreur retrieve lds_tag " + SQLCA.sqlerrtext
	lb_erreur = TRUE
END IF

DESTROY lds_tag
IF NOT lb_erreur THEN
	gu_message.uf_info(ls_message)
	return(1)
ELSE
	gu_message.uf_error(ls_message)
	return(-1)
END IF

end function

public function integer mf_exportcal ();// Export CAL_FERIES (calendrier des jours féries)
// NB : on utilise le même DS que l'objet gds_cal_feries.
uo_ds		lds_cal
string	ls_filename, ls_message
boolean	lb_erreur
long		ll_nbrows

lb_erreur = FALSE

lds_cal = CREATE uo_ds
lds_cal.dataobject = "ds_cal_feries"
lds_cal.setTransobject(SQLCA)
ll_nbrows = lds_cal.retrieve()
IF ll_nbrows > 0 THEN
	ls_filename = f_string(gs_cenpath + "\cal_feries.xml")
	IF lds_cal.saveas(ls_filename, XML!, false) = 1 THEN
		ls_message = "Fichier " + ls_filename + " créé avec succès"
	ELSE
		ls_message = "Erreur création fichier " + ls_filename
		lb_erreur = TRUE
	END IF
ELSE
	ls_message = "Erreur retrieve lds_cal " + SQLCA.sqlerrtext
	lb_erreur = TRUE
END IF

DESTROY lds_cal
IF NOT lb_erreur THEN
	gu_message.uf_info(ls_message)
	return(1)
ELSE
	gu_message.uf_error(ls_message)
	return(-1)
END IF

end function

public subroutine mf_carnet_spec ();// PCO 24/03/2017
// Il n'y a pas de "changemenu" dans le carnet de tournée, j'utilise donc cette fonction spécifique
// pour réaliser des opérations particulières de configuration du menu. 
// La fonction devra être appelée à l'ouverture de l'application

// exemple : cacher/montrer rapport en cours de développement
//IF upper(profileString(gs_locinifile, "misc", "vehiculeSurv", "FALSE")) = "TRUE" THEN
//	m_impressions.m_rpt_vehicule_surv.visible = TRUE
//ELSE
//	m_impressions.m_rpt_vehicule_surv.visible = FALSE
//END IF
IF upper(profileString(gs_locinifile, gs_username, "helpparam", "FALSE")) = "TRUE" THEN
	m_help.m_helpparampdf.visible = TRUE
ELSE
	m_help.m_helpparampdf.visible = FALSE
END IF
end subroutine

