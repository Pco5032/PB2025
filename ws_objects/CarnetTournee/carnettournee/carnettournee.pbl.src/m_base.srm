type m_exporterversxml from menu within m_param
end type

event clicked;// export V_DICOPREST (nouveau référentiel)
// 2 fichiers sont générés : une version française (v_dicoprestF.xml) et une allemande (v_dicoprestD.xml)
// PCO 19/02/2016 : 2 fichiers en + : TAGF et TAGD contenant les traductions
// PCO 09/03/2016 : 1 fichier en + : CAL_FERIES contenant le calendrier des jours fériés
string	ls_langue_save

ls_langue_save = gs_langue

// I. fichiers langue 1
//  référentiel
mf_exportdico()
//  traductions
mf_exporttag()

// II. fichiers langue 2
IF gs_langue = "F" THEN gs_langue = "D" ELSE gs_langue = "F"
w_mdiframe.wf_init_languesession()
//   référentiel
mf_exportdico()
//   traductions
mf_exporttag()

// III. Fichier indépendant de la langue
mf_exportcal()

// IV. rétablir langue initiale
gs_langue = ls_langue_save
w_mdiframe.wf_init_languesession()

end event

