this.titlegradient = true
this.toolbarstyle = contemporarytoolbar!
this.toolbartextcolor = 134217746
this.toolbarbackcolor = 67108864
this.toolbarhighlightcolor = 134217741
this.toolbargradient = true
this.bitmapgradient = true
this.m_helppdf=create m_helppdf
this.m_helpparampdf=create m_helpparampdf
this.m_environnement=create m_environnement
this.m_-12=create m_-12
this.m_apropos=create m_apropos
this.Item[UpperBound(this.Item)+1]=this.m_helppdf
this.Item[UpperBound(this.Item)+1]=this.m_helpparampdf
this.Item[UpperBound(this.Item)+1]=this.m_environnement
this.Item[UpperBound(this.Item)+1]=this.m_-12
this.Item[UpperBound(this.Item)+1]=this.m_apropos
end on

on m_help.destroy
call super::destroy
destroy(this.m_helppdf)
destroy(this.m_helpparampdf)
destroy(this.m_environnement)
destroy(this.m_-12)
destroy(this.m_apropos)
end on

