event clicked;// ouvrir le fichier d'aide en local et s'il n'est pas là, tenter le site ftp
string	ls_link, ls_filename

ls_filename = profilestring(gs_inifile, "misc", "helpPDF", "")

IF NOT f_isEmptyString(ls_filename) THEN
	ls_filename = gs_cenpath + "\" + ls_filename
	IF fileExists(ls_filename) THEN
		f_runfile(ls_filename)
		return
	END IF
END IF

ls_link = ProfileString (gs_inifile, "misc", "linkHelpPDF", "")
IF NOT f_isEmptyString(ls_filename) THEN
	f_openlink(ls_link)
END IF

gu_message.uf_info("Fichier d'aide " + ls_filename + " introuvable sur votre PC, vérifiez les téléchargements...")
return
end event

type m_helpparampdf from menu within m_help
end type

event clicked;// ouvrir le fichier d'aide au paramétrage (en local uniquement)
string	ls_filename, ls_paramName

ls_paramName = "helpParamPDF"
ls_filename = profilestring(gs_inifile, "misc", ls_paramName, "")

IF f_isEmptyString(ls_filename) THEN
	gu_message.uf_info("Nom du fichier d'aide '" + ls_paramName + "' non paramétré.")
	return
ELSE
	ls_filename = gs_cenpath + "\" + ls_filename
	IF fileExists(ls_filename) THEN
		f_runfile(ls_filename)
		return
	ELSE
		gu_message.uf_info("Fichier d'aide " + ls_filename + " introuvable sur votre PC.")
		return
	END IF
END IF
end event

