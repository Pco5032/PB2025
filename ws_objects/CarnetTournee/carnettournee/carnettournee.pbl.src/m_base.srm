event clicked;long		ll_pjob
integer	li_status
window ActiveSheet


ActiveSheet = ParentWindow.GetFirstSheet()
IF IsValid(ActiveSheet) THEN
	// déclencher l'event ue_print. S'il n'y en a pas ou s'il n'y a rien dedans, demander s'il faut imprimer la fenêtre
   IF activesheet.triggerevent("ue_print") = -1 THEN
		IF gu_message.uf_query("Imprimer", "Pas d'impression spécifique prévue dans ce contexte." + &
						"~nFaut-il imprimer une image de la fenêtre en cours ?", 2) = 1 THEN
			SetPointer(hourglass!)
			ll_pjob = PrintOpen(Activesheet.Title)
			ActiveSheet.Print(ll_pjob,200,300)
			PrintClose(ll_pjob)
		END IF
	END IF
END IF

end event

