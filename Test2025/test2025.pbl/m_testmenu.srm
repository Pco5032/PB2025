this.toolbarhighlightcolor = 134217741
this.toolbargradient = true
this.bitmapgradient = true
this.m_opensheet=create m_opensheet
this.Item[UpperBound(this.Item)+1]=this.m_opensheet
end on

on m_fichier.destroy
